{
    "compilerOptions": {
        "target": "es5",
        "strict": true,
        "module": "es6",
        "moduleResolution": "Node",
        "declaration": true,
        "outDir": "dist",
        "noImplicitReturns": true,
        "noImplicitThis": true
    },
    "include": [
        "./src/**/*.ts"
    ],
    "exclude": [
        "dist",
        "node_modules"
    ]
}